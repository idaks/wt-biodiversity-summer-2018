%%% Max Number of Euler Regions
#maxint=19.

%%% Euler Regions
r(M):- #int(M),M>=1,M<=#maxint.

%%% Concepts
tax(t2015,0).
concept2(A, B) :- concept(A,B,_).
concept(c2015_Andropogon_dealbatus,0,0).
concept(c2015_Andropogon_glomeratus_var_glomeratus,0,1).
concept(c2015_Andropogon_glaucopsis,0,2).
concept2(c2015_Andropogon_virginicus,0).
concept(c2015_Andropogon_hirsutior,0,3).
concept(c2015_Andropogon_tenuispatheus,0,4).
concept(c2015_Andropogon_capillipes,0,5).
concept(c2015_Andropogon_virginicus_smoothvariant,0,6).
concept(c2015_Andropogon_virgincus_oldfieldvariant,0,7).
concept2(c2015_Andropogon,0).
concept(c2015_Andropogon_virginicus_var_decipiens,0,8).
tax(t1968,1).
concept2(A, B) :- concept(A,B,_).
concept(c1968_Andropogn_virginicus,1,0).
concept2(c1968_Andropogn,1).

%%% Euler Bit
bit(M, 0, V):-r(M),M1=M/1, #mod(M1,10,V).
bit(M, 1, V):-r(M),M1=M/10, #mod(M1,2,V).


%%% Meaning of regions
in(X, M) :- r(M),concept(X,T,N),N1=N+1,bit(M,T,N1).
out(X, M) :- r(M),concept(X,T,N),N1=N+1,not bit(M,T,N1).
in(X, M) :- r(M),concept2(X,_),not out(X, M).
out(X, M) :- out3(X, M, _), ix.
irs(M) :- in(X, M), out(X, M), r(M), concept2(X,_).

%%% Constraints of regions.
irs(X) :- ir(X, _).
vrs(X) :- vr(X, _).
vr(X, X) :- not irs(X), r(X).
ir(X, X) :- not vrs(X), r(X), pw.
ie(prod(A,B)) :- vr(X, A), ir(X, B), ix.
:- vrs(X), irs(X), pw.

%%% Inconsistency Explanation.
ie(s(R, A, Y)) :- pie(R, A, Y), not cc(R, Y), ix.
cc(R, Y) :- c(R, _, Y), ix.

