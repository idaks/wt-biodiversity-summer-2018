%%% Max Number of Euler Regions
#maxint=55.

%%% Euler Regions
r(M):- #int(M),M>=1,M<=#maxint.

%%% Concepts
tax(t1920,0).
concept2(A, B) :- concept(A,B,_).
concept(c1920_Minyomerus_caseyi,0,0).
concept(c1920_Thylacites_microsus,0,1).
concept(c1920_Elissa_laticeps,0,2).
concept(c1920_Minyomerus_innocuus,0,3).
concept2(c1920_Minyomerus,0).
concept(c1920_Minyomerus_conicollis,0,4).
concept2(c1920_Elissa,0).
concept(c1920_Elissa_constricta,0,5).
concept(c1920_Minyomerus_languidus,0,6).
tax(t1913,1).
concept2(A, B) :- concept(A,B,_).
concept(c1913_Minyomerus_caseyi,1,0).
concept(c1913_Thylacites_microsus,1,1).
concept(c1913_Elissa_laticeps,1,2).
concept(c1913_Minyomerus_innocuus,1,3).
concept2(c1913_Minyomerus,1).
concept2(c1913_Elissa,1).
concept(c1913_Elissa_constricta,1,4).
concept(c1913_Minyomerus_languidus,1,5).

%%% Euler Bit
bit(M, 0, V):-r(M),M1=M/1, #mod(M1,8,V).
bit(M, 1, V):-r(M),M1=M/8, #mod(M1,7,V).


%%% Meaning of regions
in(X, M) :- r(M),concept(X,T,N),N1=N+1,bit(M,T,N1).
out(X, M) :- r(M),concept(X,T,N),N1=N+1,not bit(M,T,N1).
in(X, M) :- r(M),concept2(X,_),not out(X, M).
out(X, M) :- out3(X, M, _), ix.
irs(M) :- in(X, M), out(X, M), r(M), concept2(X,_).

%%% Constraints of regions.
irs(X) :- ir(X, _).
vrs(X) :- vr(X, _).
vr(X, X) :- not irs(X), r(X).
ir(X, X) :- not vrs(X), r(X), pw.
ie(prod(A,B)) :- vr(X, A), ir(X, B), ix.
:- vrs(X), irs(X), pw.

%%% Inconsistency Explanation.
ie(s(R, A, Y)) :- pie(R, A, Y), not cc(R, Y), ix.
cc(R, Y) :- c(R, _, Y), ix.

