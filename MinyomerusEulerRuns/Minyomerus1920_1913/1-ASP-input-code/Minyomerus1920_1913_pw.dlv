:- #count {X : vrs(X), in(c1920_Elissa_constricta,X), in(c1920_Elissa,X)} <= 0, pw.
pie(r0, A, 1) :- ir(X, A), in(c1920_Elissa_constricta, X), in(c1920_Elissa, X), ix.
c(r0, A, 1) :- vr(X, A), in(c1920_Elissa_constricta, X), in(c1920_Elissa, X), ix.

